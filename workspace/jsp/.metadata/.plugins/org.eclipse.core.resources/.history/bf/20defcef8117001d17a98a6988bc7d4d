	int result = 0;
	
	try {
		//FileWriter fw = new FileWriter(uploadFile);	//write
		FileWriter fw = new FileWriter(uploadFile, true);	//append
		fw.write(msg);
		fw.close();
		result++;
	} catch(Exception e) {
		//e.printStackTrace();
		result = 0;
	}
	
	
%>